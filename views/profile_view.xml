<?xml version='1.0' encoding='utf-8'?>
<odoo>
  <record id="profile_tree_view" model="ir.ui.view">
    <field name="name">My Profile</field>
    <field name="model">myprofile</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="state"/>
      </tree>
    </field>
  </record>


  <record id="profile_form_view" model="ir.ui.view">
    <field name="name">My Profile</field>
    <field name="model">myprofile</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
          </group>

          <group>
            <group>
              <field name="name"/>
            </group>
            <group>
              <field name="state"/>
            </group>
          </group>
                
          <notebook>
            <page string="Details">
              <group>
                <field name="name"/>
              </group>
            </page>
            <page string="Data">
              <group>
                <field name="name"/>
              </group>
            </page>
          </notebook>
        </sheet>  
      </form>
    </field>
  </record>
</odoo>